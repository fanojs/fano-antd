{
  "name": "fano-antd",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib --source-maps --extensions .es6,.es,.jsx,.js --copy-files",
    "watch": "babel src --watch --out-dir lib --source-maps --extensions .es6,.es,.jsx,.js --copy-files",
    "lint": "standard --verbose",
    "fix": "standard --fix",
    "pre-commit": "npm run lint",
    "pub": "BABEL_ENV=production npm run build",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanojs/react-antd.git"
  },
  "keywords": [],
  "author": "Daniel Yin <yinfxs@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/fanojs/react-antd/issues"
  },
  "homepage": "https://github.com/fanojs/react-antd#readme",
  "dependencies": {
    "antd": "^3.10.7",
    "array-to-tree": "^3.3.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "qs": "^6.5.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-resizable": "^1.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-react": "^6.24.1",
    "husky": "^1.1.4",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "transform-es2015-modules-umd",
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": true
        }
      ]
    ],
    "env": {
      "production": {
        "presets": [
          "minify",
          "env",
          "react"
        ]
      }
    }
  }
}
